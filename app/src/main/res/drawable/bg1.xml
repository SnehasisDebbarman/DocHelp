<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="135"
            android:toDegrees="135"
            android:pivotX="20%"
            android:pivotY="20%" >
            <shape
                android:shape="oval" >
                <stroke
                    android:width="1dip"
                    android:color="@color/blue" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="80"
            android:pivotX="20%"
            android:pivotY="20%" >
            <shape
                android:shape="oval" >
                <stroke
                    android:width="1dip"
                    android:color="@color/blue" />
            </shape>
        </rotate>

    </item>
</layer-list>